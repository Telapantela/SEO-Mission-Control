{
	"version": 3,
	"routes": [
		{
			"src": "^/\\.well-known(?:/.*)?$"
		},
		{
			"src": "^/((?:[^/]+/)*[^/\\.]+)$",
			"headers": {
				"Location": "/$1/"
			},
			"status": 308
		},
		{
			"src": "^/((?:[^/]+/)*[^/]+\\.\\w+)/$",
			"headers": {
				"Location": "/$1"
			},
			"status": 308
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/$",
			"dest": "_render"
		},
		{
			"src": "^/_image/$",
			"dest": "_render"
		},
		{
			"src": "^/blog/$",
			"dest": "_render"
		},
		{
			"src": "^/blog(?:/(.*?))?/$",
			"dest": "_render"
		},
		{
			"src": "^/legal(?:/(.*?))?/$",
			"dest": "_render"
		},
		{
			"src": "^/projects/$",
			"dest": "_render"
		},
		{
			"src": "^/projects(?:/(.*?))?/$",
			"dest": "_render"
		},
		{
			"src": "^/robots\\.txt/?$",
			"dest": "_render"
		},
		{
			"src": "^/rss\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/search/$",
			"dest": "_render"
		},
		{
			"src": "^/work/$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}